#cal-heatmap
  :javascript 
    var cal = new CalHeatMap();
      cal.init({
        //data: @timestamps,
        domainLabelFormat: "%b",
        id : "cal-heatmap",
        domain : "month",
        subDomain : "day",
        range : 12 , 
        legendColors: ["#efefef", "steelblue"],
        tooltip: true,
        colLimit: 4,
        label: { position: "top" },
        domainMargin: 1,
        scale: [2, 5, 8, 10]
      });
  

