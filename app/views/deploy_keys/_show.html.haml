%tr
  %td
    %a{:href => project_deploy_key_path(key.project, key)}
      %strong= key.title
  %td
    %span.update-author
      = t '.added'
      = time_ago_in_words(key.created_at)
      = t 'global.ago'
  %td
    = link_to t('global.destroy'), project_deploy_key_path(key.project, key), :confirm => t('global.destroy_confirm'), :method => :delete, :class => "danger btn delete-key small right"

