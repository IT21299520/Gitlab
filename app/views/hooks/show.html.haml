= render "repositories/head"
%pre= @hook.url

- if can? current_user, :admin_project, @project
  .actions
    = link_to t('.test'), test_project_hook_path(@project, @hook), :class => "btn"
    = link_to t('global.destroy'), project_hook_path(@project, @hook), :confirm => t('global.destroy_confirm'), :method => :delete, :class => "danger btn"
